{
	"key": "global",
	"breakpoints": ["", "sm", "md", "lg", "xl", "xxl"],
	"breakpointsDelimiter": ":",
	"prefixDelimiter": "-",
	"frames": {
		"part": "frame-group",
		"fields": [
			{
				"obj": { 
					"icon": "deviceOff",
					"label": { "title": "off ⇧⌘1" },
					"input": { 
						"data-dimensions": "",
						"data-sr": "",
						"name": "frames",
						"type": "radio",
						"value": "",
						"checked": ""
					}
				}
			},
			
			{
				"obj": { "icon": "deviceSM", "label": { "title": "off ⇧⌘1" }, "input": { "data-dimensions": "360x640", "data-sr":"", "name":"frames", "type":"radio", "value": "sm" } }
			},
			{
				"obj": { "icon": "deviceMD", "label": { "title": "off ⇧⌘1" }, "input": { "data-dimensions": "768x1024", "data-sr":"", "name":"frames", "type":"radio", "value": "md" } }
			},
			{
				"obj": { "icon": "deviceLG", "label": { "title": "off ⇧⌘1" }, "input": { "data-dimensions": "1024x1280", "data-sr":"", "name":"frames", "type":"radio", "value": "lg" } }
			},
			{
				"obj": { "icon": "deviceXL", "label": { "title": "off ⇧⌘1" }, "input": { "data-dimensions": "1280x1280", "data-sr":"", "name":"frames", "type":"radio", "value": "xl" } }
			},
			{
				"obj": { "icon": "device2XL", "label": { "title": "off ⇧⌘1" }, "input": { "data-dimensions": "1536x1280", "data-sr":"", "name":"frames", "type":"radio", "value": "2xl" } }
			}	
		]
	},
	"header": [
		{
			"ui": "button",
			"obj": {
				"data-click": "colorscheme",
				"icon": "colorscheme",
				"name": "colorscheme",
				"title": "Toggle Color Scheme"
			}
		},
		{
			"ui": "tag",
			"tag": "h1",
			"content": "Editor",
			"attributes": {
				"part": "title draghandle"
			}
		},
		{
			"ui": "button",
			"obj": { 
				"data-click": "close", 
				"icon": "close", 
				"name": "close", 
				"part": "close", 
				"title": "Close Editor"
			}
		}
	],
	"footer": {
		"label": "Frame",
		"name": "frame-root",
		"open": true,
		"part": "appgroup",
		"fieldsets": [
			{
				"name": "frameinfo",
				"part": "frameinfo",
				"fields": [
					{
						"ui": "output",
						"name": "x",
						"text": "X"
					},
					{
						"ui": "output",
						"name": "y",
						"text": "Y"
					},
					{
						"ui": "output",
						"name": "w",
						"text": "W"
					},
					{
						"ui": "output",
						"name": "h",
						"text": "H"
					},
					{
						"ui": "output",
						"name": "tag",
						"text": "E"
					},
					{
						"ui": "output",
						"name": "parent",
						"text": "P"
					}
				]
			},
			{
				"name": "dom-navigation",
				"part": "group",
				"fields": [
					{ 
						"ui": "button",
						"obj": {
							"data-click": "nav-up",
							"icon": "up",
							"title": "Parent ⌘↑"
						}
					},
					{ 
						"ui": "button",
						"obj": {
							"data-click": "nav-down",
							"icon": "down",
							"title": "Child ⌘↓"
						}
					},
					{ 
						"ui": "button",
						"obj": {
							"data-click": "nav-left",
							"icon": "left",
							"title": "Previous Sibling ⌘←"
						}
					},
					{ 
						"ui": "button",
						"obj": {
							"data-click": "nav-right",
							"icon": "right",
							"title": "Next Sibling ⌘→"
						}
					},
					{ 
						"ui": "button",
						"obj": {
							"data-click": "nav-first",
							"icon": "first",
							"title": "First Child ⇧⌘←"
						}
					},
					{ 
						"ui": "button",
						"obj": {
							"data-click": "nav-last",
							"icon": "last",
							"title": "Last Child ⇧⌘→"
						}
					},
					{ 
						"ui": "button",
						"obj": {
							"data-click": "dom-undo",
							"icon": "undo",
							"part": "secondary",
							"title": "Undo  ⌘Z"
						}
					},
					{ 
						"ui": "button",
						"obj": {
							"data-click": "dom-redo",
							"icon": "redo",
							"part": "secondary",
							"title": "Redo ⇧⌘Z"
						}
					}
				]
			}
		]
	}
}