  "rateCategories": {
    "rate1": {
      "description": "Standard rate for passenger vehicles on local roads (under 65 mph)",
      "conditions": {
        "speedLimit": "< 65",
        "vehicle": ["car", "mc"],
        "trailer": false
      }
    },
    "rate2": {
      "description": "Highway rate for passenger vehicles (65+ mph speeds)", 
      "conditions": {
        "speedLimit": ">= 65",
        "vehicle": ["car", "mc"],
        "trailer": false
      }
    },
    "rate3": {
      "description": "Commercial vehicles, trucks, buses, and vehicles with trailers",
      "conditions": {
        "vehicle": ["truck", "commercial", "bus"],
        "trailer": true
      }
    }
  },
  "consequenceTypes": {
    "LICENSE_SUSPENSION": {
      "id": "LICENSE_SUSPENSION",
      "severity": 5,
      "messageKey": "license_suspension",
      "preventsFine": false
    },
    "COURT_APPEARANCE": {
      "id": "COURT_APPEARANCE", 
      "severity": 4,
      "messageKey": "court_required",
      "preventsFine": false
    },
    "LICENSE_POINTS": {
      "id": "LICENSE_POINTS",
      "severity": 3,
      "messageKey": "points_added",
      "preventsFine": false
    },
    "CRIMINAL_CHARGES": {
      "id": "CRIMINAL_CHARGES",
      "severity": 6,
      "messageKey": "criminal_charges",
      "preventsFine": false
    },
    "LICENSE_REVOCATION": {
      "id": "LICENSE_REVOCATION",
      "severity": 7,
      "messageKey": "license_revoked",
      "preventsFine": false
    }
  },
  "ruleEngine": {
    "rateSelectionRules": [
      {
        "id": "rate3_vehicles",
        "description": "Commercial vehicles, trucks, buses, or vehicles with trailers",
        "conditions": [
          {
            "type": "or",
            "rules": [
              { "field": "vehicle.category", "operator": "in", "value": ["truck", "commercial", "bus"] },
              { "field": "factors", "operator": "includes", "value": "towing" }
            ]
          }
        ],
        "result": "rate3"
      },
      {
        "id": "rate2_highway",
        "description": "Highway speeds (65+ mph) for passenger vehicles and motorcycles",
        "conditions": [
          { "field": "speedLimit", "operator": ">=", "value": 65 },
          { "field": "vehicle.category", "operator": "in", "value": ["car", "mc"] },
          { "field": "factors", "operator": "not_includes", "value": "towing" }
        ],
        "result": "rate2"
      },
      {
        "id": "rate1_default",
        "description": "Standard rate for local roads and lower speed limits",
        "conditions": [],
        "result": "rate1"
      }
    ],
    "penaltyRules": [
      {
        "id": "school_work_zone_penalty",
        "description": "Double fine in active school zones or work zones with workers present",
        "conditions": [
          {
            "type": "or",
            "rules": [
              { "field": "factors", "operator": "includes", "value": "schoolZone" },
              { "field": "factors", "operator": "includes", "value": "workZone" }
            ]
          },
          { "field": "percentageOver", "operator": ">=", "value": 5 }
        ],
        "penalty": 100
      },
      {
        "id": "extreme_speed_penalty",
        "description": "Additional penalty for speeds over 100 mph on any road",
        "conditions": [
          { "field": "speed", "operator": ">=", "value": 100 }
        ],
        "formula": {
          "type": "calculation",
          "expression": "Math.floor((speed - 100) / 5) * 100 + 300"
        }
      },
      {
        "id": "urban_excessive_speed_penalty",
        "description": "Enhanced penalty for dangerous speeds in urban areas",
        "conditions": [
          { "field": "roadType", "operator": "=", "value": "urban" },
          { "field": "percentageOver", "operator": ">=", "value": 50 }
        ],
        "penalty": 150
      },
      {
        "id": "repeat_offender_penalty",
        "description": "Enhanced penalty for repeat offenders",
        "conditions": [
          { "field": "factors", "operator": "includes", "value": "priorOffenses" },
          { "field": "percentageOver", "operator": ">=", "value": 20 }
        ],
        "penalty": 200
      }
    ],
    "consequenceRules": [
      {
        "id": "criminal_charges_extreme",
        "description": "Criminal charges for extreme speeding (120+ mph or 50+ over)",
        "conditions": [
          {
            "type": "or",
            "rules": [
              { "field": "speed", "operator": ">=", "value": 120 },
              {
                "type": "and",
                "rules": [
                  { "field": "percentageOver", "operator": ">=", "value": 140 },
                  { "field": "speed", "operator": ">=", "value": 90 }
                ]
              }
            ]
          }
        ],
        "consequence": "CRIMINAL_CHARGES"
      },
      {
        "id": "license_revocation_extreme",
        "description": "License revocation for three violations in 18 months",
        "conditions": [
          {
            "type": "and",
            "rules": [
              { "field": "factors", "operator": "includes", "value": "priorOffenses" },
              { "field": "percentageOver", "operator": ">=", "value": 60 }
            ]
          }
        ],
        "consequence": "LICENSE_REVOCATION"
      },
      {
        "id": "license_suspension_high_speed",
        "description": "License suspension for dangerous speeds (100+ mph or 40+ over)",
        "conditions": [
          {
            "type": "or",
            "rules": [
              { "field": "speed", "operator": ">=", "value": 100 },
              {
                "type": "and",
                "rules": [
                  { "field": "percentageOver", "operator": ">=", "value": 100 },
                  { "field": "speed", "operator": ">=", "value": 85 }
                ]
              }
            ]
          }
        ],
        "consequence": "LICENSE_SUSPENSION"
      },
      {
        "id": "court_appearance_required",
        "description": "Mandatory court appearance for serious violations",
        "conditions": [
          {
            "type": "or",
            "rules": [
              { "field": "percentageOver", "operator": ">=", "value": 100 },
              {
                "type": "and",
                "rules": [
                  { "field": "factors", "operator": "includes", "value": "recklessDriving" },
                  { "field": "percentageOver", "operator": ">=", "value": 60 }
                ]
              },
              {
                "type": "and",
                "rules": [
                  { "field": "factors", "operator": "includes", "value": "schoolZone" },
                  { "field": "percentageOver", "operator": ">=", "value": 40 }
                ]
              },
              {
                "type": "and",
                "rules": [
                  { "field": "factors", "operator": "includes", "value": "workZone" },
                  { "field": "percentageOver", "operator": ">=", "value": 40 }
                ]
              }
            ]
          }
        ],
        "consequence": "COURT_APPEARANCE"
      },
      {
        "id": "license_points_all",
        "description": "License points for all speeding violations",
        "conditions": [
          { "field": "percentageOver", "operator": ">=", "value": 0 }
        ],
        "consequence": "LICENSE_POINTS"
      }
    ]
  },
