  "vehicles": {
    "car": {
      "id": "car",
      "label": "Passenger Vehicle",
      "category": "car"
    },
    "motorcycle": {
      "id": "motorcycle",
      "label": "Motorcycle",
      "category": "mc"
    },
    "truck": {
      "id": "truck",
      "label": "Truck/RV",
      "category": "truck"
    },
    "commercial": {
      "id": "commercial",
      "label": "Commercial Vehicle",
      "category": "commercial"
    },
    "schoolBus": {
      "id": "schoolBus",
      "label": "School Bus",
      "category": "bus"
    }
  },
  "factors": {
    "schoolZone": {
      "id": "schoolZone",
      "label": "School Zone",
      "description": "Speeding in an active school zone (doubled fines)",
      "multiplier": 2.0,
      "visibleFor": ["car", "mc", "truck", "commercial", "bus"]
    },
    "workZone": {
      "id": "workZone",
      "label": "Work/Construction Zone",
      "description": "Speeding in an active work zone with workers present (doubled fines)",
      "multiplier": 2.0,
      "visibleFor": ["car", "mc", "truck", "commercial", "bus"]
    },
    "recklessDriving": {
      "id": "recklessDriving",
      "label": "Reckless Driving",
      "description": "Excessive speeding with dangerous behavior",
      "multiplier": 1.5,
      "visibleFor": ["car", "mc"]
    },
    "youthfulDriver": {
      "id": "youthfulDriver",
      "label": "Driver Under 21",
      "description": "Enhanced penalties for drivers under 21",
      "multiplier": 1.2,
      "visibleFor": ["car", "mc"]
    },
    "towing": {
      "id": "towing",
      "label": "Towing Trailer",
      "description": "Vehicle towing a trailer or mobile home",
      "multiplier": 1.0,
      "visibleFor": ["car", "mc", "truck"]
    },
    "priorOffenses": {
      "id": "priorOffenses",
      "label": "Repeat Offender",
      "description": "Multiple speeding violations in 18 months",
      "multiplier": 1.5,
      "visibleFor": ["car", "mc", "truck", "commercial", "bus"]
    }
  },
  "penaltyRanges": [
    {
      "percentageOver": 0,
      "rate1": 95,
      "rate2": 125, 
      "rate3": 175,
      "description": "Minor speeding (1-10 mph over)",
      "consequence": "Fine + 3 points",
      "summary": "Minor violation"
    },
    {
      "percentageOver": 20,
      "rate1": 150,
      "rate2": 190,
      "rate3": 240,
      "description": "Moderate speeding (11-20 mph over)", 
      "consequence": "Fine + 4 points",
      "summary": "Moderate violation"
    },
    {
      "percentageOver": 40,
      "rate1": 240,
      "rate2": 320,
      "rate3": 420,
      "description": "Serious speeding (21-30 mph over)",
      "consequence": "High fine + 6 points",
      "summary": "Serious violation"
    },
    {
      "percentageOver": 60,
      "rate1": 380,
      "rate2": 520, 
      "rate3": 680,
      "description": "Severe speeding (31-40 mph over)",
      "consequence": "Very high fine + 8 points",
      "summary": "Severe violation"
    },
    {
      "percentageOver": 80,
      "rate1": 580,
      "rate2": 780,
      "rate3": 980,
      "description": "Extreme speeding (41+ mph over)",
      "consequence": "Maximum fine + 11 points + license review",
      "summary": "Extreme violation"
    },
    {
      "percentageOver": 100,
      "rate1": 800,
      "rate2": 1150,
      "rate3": 1500,
      "description": "Reckless speeding (50+ mph over)", 
      "consequence": "Court appearance + possible license suspension",
      "summary": "Reckless driving"
    },
    {
      "percentageOver": 140,
      "rate1": 1200,
      "rate2": 1600,
      "rate3": 2000,
      "description": "Criminal speeding (excessive speed)",
      "consequence": "Criminal charges + license suspension",
      "summary": "Criminal violation"
    }
  ],
  "rateCategories": {
    "rate1": {
      "description": "Standard rate for passenger vehicles on local roads (under 65 mph)",
      "conditions": {
        "speedLimit": "< 65",
        "vehicle": ["car", "mc"],
        "trailer": false
      }
    },
    "rate2": {
      "description": "Highway rate for passenger vehicles (65+ mph speeds)", 
      "conditions": {
        "speedLimit": ">= 65",
        "vehicle": ["car", "mc"],
        "trailer": false
      }
    },
    "rate3": {
      "description": "Commercial vehicles, trucks, buses, and vehicles with trailers",
      "conditions": {
        "vehicle": ["truck", "commercial", "bus"],
        "trailer": true
      }
    }
  },
  "consequenceTypes": {
    "LICENSE_SUSPENSION": {
      "id": "LICENSE_SUSPENSION",
      "severity": 5,
      "messageKey": "license_suspension",
      "preventsFine": false
    },
    "COURT_APPEARANCE": {
      "id": "COURT_APPEARANCE", 
      "severity": 4,
      "messageKey": "court_required",
      "preventsFine": false
    },
    "LICENSE_POINTS": {
      "id": "LICENSE_POINTS",
      "severity": 3,
      "messageKey": "points_added",
      "preventsFine": false
    },
    "CRIMINAL_CHARGES": {
      "id": "CRIMINAL_CHARGES",
      "severity": 6,
      "messageKey": "criminal_charges",
      "preventsFine": false
    },
    "LICENSE_REVOCATION": {
      "id": "LICENSE_REVOCATION",
      "severity": 7,
      "messageKey": "license_revoked",
      "preventsFine": false
    }
  },
  "ruleEngine": {
    "rateSelectionRules": [
      {
        "id": "rate3_vehicles",
        "description": "Commercial vehicles, trucks, buses, or vehicles with trailers",
        "conditions": [
          {
            "type": "or",
            "rules": [
              { "field": "vehicle.category", "operator": "in", "value": ["truck", "commercial", "bus"] },
              { "field": "factors", "operator": "includes", "value": "towing" }
            ]
          }
        ],
        "result": "rate3"
      },
      {
        "id": "rate2_highway",
        "description": "Highway speeds (65+ mph) for passenger vehicles and motorcycles",
        "conditions": [
          { "field": "speedLimit", "operator": ">=", "value": 65 },
          { "field": "vehicle.category", "operator": "in", "value": ["car", "mc"] },
          { "field": "factors", "operator": "not_includes", "value": "towing" }
        ],
        "result": "rate2"
      },
      {
        "id": "rate1_default",
        "description": "Standard rate for local roads and lower speed limits",
        "conditions": [],
        "result": "rate1"
      }
    ],
    "penaltyRules": [
      {
        "id": "school_work_zone_penalty",
        "description": "Double fine in active school zones or work zones with workers present",
        "conditions": [
          {
            "type": "or",
            "rules": [
              { "field": "factors", "operator": "includes", "value": "schoolZone" },
              { "field": "factors", "operator": "includes", "value": "workZone" }
            ]
          },
          { "field": "percentageOver", "operator": ">=", "value": 5 }
        ],
        "penalty": 100
      },
      {
        "id": "extreme_speed_penalty",
        "description": "Additional penalty for speeds over 100 mph on any road",
        "conditions": [
          { "field": "speed", "operator": ">=", "value": 100 }
        ],
        "formulaId": "extremeSpeedPenalty"
      },
      {
        "id": "urban_excessive_speed_penalty",
        "description": "Enhanced penalty for dangerous speeds in urban areas",
        "conditions": [
          { "field": "roadType", "operator": "=", "value": "urban" },
          { "field": "percentageOver", "operator": ">=", "value": 50 }
        ],
        "penalty": 150
      },
      {
        "id": "repeat_offender_penalty",
        "description": "Enhanced penalty for repeat offenders",
        "conditions": [
          { "field": "factors", "operator": "includes", "value": "priorOffenses" },
          { "field": "percentageOver", "operator": ">=", "value": 20 }
        ],
        "penalty": 200
      }
    ],
    "consequenceRules": [
      {
        "id": "criminal_charges_extreme",
        "description": "Criminal charges for extreme speeding (120+ mph or 50+ over)",
        "conditions": [
          {
            "type": "or",
            "rules": [
              { "field": "speed", "operator": ">=", "value": 120 },
              {
                "type": "and",
                "rules": [
                  { "field": "percentageOver", "operator": ">=", "value": 140 },
                  { "field": "speed", "operator": ">=", "value": 90 }
                ]
              }
            ]
          }
        ],
        "consequence": "CRIMINAL_CHARGES"
      },
      {
        "id": "license_revocation_extreme",
        "description": "License revocation for three violations in 18 months",
        "conditions": [
          {
            "type": "and",
            "rules": [
              { "field": "factors", "operator": "includes", "value": "priorOffenses" },
              { "field": "percentageOver", "operator": ">=", "value": 60 }
            ]
          }
        ],
        "consequence": "LICENSE_REVOCATION"
      },
      {
        "id": "license_suspension_high_speed",
        "description": "License suspension for dangerous speeds (100+ mph or 40+ over)",
        "conditions": [
          {
            "type": "or",
            "rules": [
              { "field": "speed", "operator": ">=", "value": 100 },
              {
                "type": "and",
                "rules": [
                  { "field": "percentageOver", "operator": ">=", "value": 100 },
                  { "field": "speed", "operator": ">=", "value": 85 }
                ]
              }
            ]
          }
        ],
        "consequence": "LICENSE_SUSPENSION"
      },
      {
        "id": "court_appearance_required",
        "description": "Mandatory court appearance for serious violations",
        "conditions": [
          {
            "type": "or",
            "rules": [
              { "field": "percentageOver", "operator": ">=", "value": 100 },
              {
                "type": "and",
                "rules": [
                  { "field": "factors", "operator": "includes", "value": "recklessDriving" },
                  { "field": "percentageOver", "operator": ">=", "value": 60 }
                ]
              },
              {
                "type": "and",
                "rules": [
                  { "field": "factors", "operator": "includes", "value": "schoolZone" },
                  { "field": "percentageOver", "operator": ">=", "value": 40 }
                ]
              },
              {
                "type": "and",
                "rules": [
                  { "field": "factors", "operator": "includes", "value": "workZone" },
                  { "field": "percentageOver", "operator": ">=", "value": 40 }
                ]
              }
            ]
          }
        ],
        "consequence": "COURT_APPEARANCE"
      },
      {
        "id": "license_points_all",
        "description": "License points for all speeding violations",
        "conditions": [
          { "field": "percentageOver", "operator": ">=", "value": 0 }
        ],
        "consequence": "LICENSE_POINTS"
      }
    ]
  },
