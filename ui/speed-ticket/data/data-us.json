  "speedLimitRules": [
    {
      "id": "towing_vehicles",
      "description": "Vehicles towing trailers (all roads) - reduced limits",
      "priority": 1,
      "conditions": [
        { "field": "factors", "operator": "includes", "value": "towing" }
      ],
      "limits": {
        "urban": [25, 30, 35, 40],
        "suburban": [35, 40, 45, 50],
        "rural": [45, 50, 55, 60, 65],
        "interstate": [55, 60, 65],
        "default": {
          "urban": 35,
          "suburban": 45,
          "rural": 55,
          "interstate": 65
        }
      }
    },
    {
      "id": "school_bus_all",
      "description": "School buses - maximum 55 mph on highways, 60 mph on interstates",
      "priority": 2,
      "conditions": [
        { "field": "vehicle.category", "operator": "=", "value": "bus" }
      ],
      "limits": {
        "urban": [25, 30, 35, 40, 45],
        "suburban": [35, 40, 45],
        "rural": [45, 50, 55],
        "interstate": [55, 60],
        "default": {
          "urban": 35,
          "suburban": 40,
          "rural": 45,
          "interstate": 60
        }
      }
    },
    {
      "id": "truck_interstate",
      "description": "Trucks on interstate highways - varies by state, typically 65-75 mph max",
      "priority": 2,
      "conditions": [
        { "field": "vehicle.category", "operator": "=", "value": "truck" },
        { "field": "roadType", "operator": "=", "value": "interstate" }
      ],
      "limits": {
        "interstate": [55, 60, 65, 70, 75],
        "default": {
          "interstate": 65
        }
      }
    },
    {
      "id": "commercial_interstate",
      "description": "Commercial vehicles on interstate highways",
      "priority": 2,
      "conditions": [
        { "field": "vehicle.category", "operator": "=", "value": "commercial" },
        { "field": "roadType", "operator": "=", "value": "interstate" }
      ],
      "limits": {
        "interstate": [55, 60, 65, 70, 75],
        "default": {
          "interstate": 65
        }
      }
    },
    {
      "id": "truck_commercial_local",
      "description": "Trucks and commercial vehicles on local roads",
      "priority": 5,
      "conditions": [
        { "field": "vehicle.category", "operator": "in", "value": ["truck", "commercial"] },
        { "field": "roadType", "operator": "in", "value": ["urban", "suburban", "rural"] }
      ],
      "limits": {
        "urban": [25, 30, 35, 40, 45],
        "suburban": [35, 40, 45, 50, 55],
        "rural": [45, 50, 55, 60, 65, 70],
        "default": {
          "urban": 35,
          "suburban": 45,
          "rural": 55
        }
      }
    },
    {
      "id": "passenger_default",
      "description": "Passenger vehicles (cars, motorcycles) - standard limits",
      "priority": 10,
      "conditions": [
        { "field": "vehicle.category", "operator": "in", "value": ["car", "mc"] }
      ],
      "limits": {
        "urban": [25, 30, 35, 40, 45],
        "suburban": [35, 40, 45, 50, 55],
        "rural": [45, 50, 55, 60, 65, 70],
        "interstate": [55, 60, 65, 70, 75, 80, 85],
        "default": {
          "urban": 35,
          "suburban": 45,
          "rural": 55,
          "interstate": 70
        }
      }
    }
  ],
  "formulas": {
    "extremeSpeedPenalty": {
      "id": "extremeSpeedPenalty",
      "type": "escalatingPenalty",
      "description": "Escalating penalty for speeds over 100 mph",
      "baseSpeed": 100,
      "increment": 5,
      "multiplier": 100,
      "base": 300
    }
  }
