  "rateCategories": {
    "rate1": {
      "description": "Standard rate for passenger vehicles on local roads",
      "conditions": {
        "speedLimit": "< 65",
        "vehicle": ["car", "mc"],
        "trailer": false
      }
    },
    "rate2": {
      "description": "Higher rate for highway speeds", 
      "conditions": {
        "speedLimit": ">= 65",
        "vehicle": ["car", "mc"],
        "trailer": false
      }
    },
    "rate3": {
      "description": "Commercial vehicle and trailer rate",
      "conditions": {
        "vehicle": ["truck", "commercial", "bus"],
        "trailer": true
      }
    }
  },
  "consequenceTypes": {
    "LICENSE_SUSPENSION": {
      "id": "LICENSE_SUSPENSION",
      "severity": 5,
      "messageKey": "license_suspension",
      "preventsFine": false
    },
    "COURT_APPEARANCE": {
      "id": "COURT_APPEARANCE", 
      "severity": 4,
      "messageKey": "court_required",
      "preventsFine": false
    },
    "LICENSE_POINTS": {
      "id": "LICENSE_POINTS",
      "severity": 3,
      "messageKey": "points_added",
      "preventsFine": false
    },
    "CRIMINAL_CHARGES": {
      "id": "CRIMINAL_CHARGES",
      "severity": 6,
      "messageKey": "criminal_charges",
      "preventsFine": false
    }
  },
  "ruleEngine": {
    "rateSelectionRules": [
      {
        "id": "rate3_vehicles",
        "description": "Commercial vehicles, trucks, buses, or vehicles with trailers",
        "conditions": [
          {
            "type": "or",
            "rules": [
              { "field": "vehicle.category", "operator": "in", "value": ["truck", "commercial", "bus"] },
              { "field": "factors", "operator": "includes", "value": "trailer" }
            ]
          }
        ],
        "result": "rate3"
      },
      {
        "id": "rate2_highway",
        "description": "Highway speeds for cars and motorcycles",
        "conditions": [
          { "field": "speedLimit", "operator": ">=", "value": 65 },
          { "field": "vehicle.category", "operator": "in", "value": ["car", "mc"] },
          { "field": "factors", "operator": "not_includes", "value": "trailer" }
        ],
        "result": "rate2"
      },
      {
        "id": "rate1_default",
        "description": "Standard rate for local roads",
        "conditions": [],
        "result": "rate1"
      }
    ],
    "penaltyRules": [
      {
        "id": "school_construction_penalty",
        "description": "Double fine in school or construction zones",
        "conditions": [
          {
            "type": "or",
            "rules": [
              { "field": "factors", "operator": "includes", "value": "schoolZone" },
              { "field": "factors", "operator": "includes", "value": "constructionZone" }
            ]
          },
          { "field": "percentageOver", "operator": ">=", "value": 10 }
        ],
        "penalty": 150
      },
      {
        "id": "extreme_speed_penalty",
        "description": "Additional penalty for speeds over 90 mph",
        "conditions": [
          { "field": "speed", "operator": ">=", "value": 90 }
        ],
        "formula": {
          "type": "calculation",
          "expression": "Math.floor((speed - 90) / 5) * 75 + 200"
        }
      },
      {
        "id": "urban_high_speed_penalty",
        "description": "Extra penalty for high speeds in urban areas",
        "conditions": [
          { "field": "roadType", "operator": "=", "value": "urban" },
          { "field": "percentageOver", "operator": ">=", "value": 40 }
        ],
        "penalty": 100
      }
    ],
    "consequenceRules": [
      {
        "id": "criminal_charges_extreme",
        "description": "Criminal charges for extreme speeding",
        "conditions": [
          {
            "type": "or",
            "rules": [
              { "field": "speed", "operator": ">=", "value": 120 },
              {
                "type": "and",
                "rules": [
                  { "field": "percentageOver", "operator": ">=", "value": 90 },
                  { "field": "speed", "operator": ">=", "value": 85 }
                ]
              }
            ]
          }
        ],
        "consequence": "CRIMINAL_CHARGES"
      },
      {
        "id": "license_suspension_high_speed",
        "description": "License suspension for very high speeds",
        "conditions": [
          {
            "type": "or",
            "rules": [
              { "field": "speed", "operator": ">=", "value": 100 },
              {
                "type": "and",
                "rules": [
                  { "field": "percentageOver", "operator": ">=", "value": 75 },
                  { "field": "speed", "operator": ">=", "value": 75 }
                ]
              }
            ]
          }
        ],
        "consequence": "LICENSE_SUSPENSION"
      },
      {
        "id": "court_appearance_required",
        "description": "Court appearance required for serious violations",
        "conditions": [
          {
            "type": "or",
            "rules": [
              { "field": "percentageOver", "operator": ">=", "value": 75 },
              {
                "type": "and",
                "rules": [
                  { "field": "factors", "operator": "includes", "value": "recklessDriving" },
                  { "field": "percentageOver", "operator": ">=", "value": 45 }
                ]
              },
              {
                "type": "and",
                "rules": [
                  { "field": "factors", "operator": "includes", "value": "schoolZone" },
                  { "field": "percentageOver", "operator": ">=", "value": 30 }
                ]
              }
            ]
          }
        ],
        "consequence": "COURT_APPEARANCE"
      },
      {
        "id": "license_points_moderate",
        "description": "License points for moderate violations",
        "conditions": [
          { "field": "percentageOver", "operator": ">=", "value": 15 }
        ],
        "consequence": "LICENSE_POINTS"
      }
    ]
  },
